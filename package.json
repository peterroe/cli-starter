{
  "name": "cli-starter",
  "version": "1.0.0",
  "packageManager": "pnpm@7.4.1",
  "description": "cli-template",
  "keywords": [
    "cli",
    "typescript",
    "template"
  ],
  "license": "MIT",
  "author": "peterroe",
  "module": "./dist/index.mjs",
  "main": "./dist/index.mjs",
  "bin": {
    "cli-starter": "./dist/index.mjs"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint \"**/*.{ts,js}\"",
    "fix": "eslint --fix \"**/*.{ts,js}\"",
    "start": "esno src/index.ts",
    "test": "vitest",
    "release": "np"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.25.2",
    "cac": "6.7.12",
    "eslint": "8.18.0",
    "esno": "0.16.3",
    "husky": "4.3.8",
    "np": "7.6.2",
    "typescript": "4.7.4",
    "unbuild": "0.7.4",
    "vite": "2.9.13",
    "vitest": "0.16.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
