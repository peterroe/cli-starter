{
  "name": "cli-starter",
  "version": "1.0.0",
  "packageManager": "pnpm@7.9.0",
  "description": "A command line tool template",
  "author": "peterroe",
  "license": "MIT",
  "homepage": "https://github.com/peterroe/cli-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterroe/cli-starter.git"
  },
  "bugs": "https://github.com/peterroe/cli-starter/issues",
  "keywords": [
    "cli",
    "typescript",
    "template"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "cli-starter": "./dist/index.mjs"
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint \"**/*.{ts,js}\"",
    "fix": "eslint --fix \"**/*.{ts,js}\"",
    "start": "esno src/index.ts",
    "test": "vitest",
    "release": "bumpp",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.25.2",
    "@types/node": "18.6.0",
    "bumpp": "8.2.1",
    "cac": "6.7.12",
    "eslint": "8.21.0",
    "esno": "0.16.3",
    "husky": "4.3.8",
    "typescript": "4.7.4",
    "unbuild": "0.8.8",
    "vite": "3.0.5",
    "vitest": "0.21.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run typecheck"
    }
  }
}
