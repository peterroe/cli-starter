{
  "name": "cli-starter",
  "version": "1.0.0",
  "packageManager": "pnpm@7.5.2",
  "description": "cli-template",
  "keywords": [
    "cli",
    "typescript",
    "template"
  ],
  "license": "MIT",
  "author": "peterroe",
  "module": "./dist/index.mjs",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "cli-starter": "./dist/index.mjs"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint \"**/*.{ts,js}\"",
    "fix": "eslint --fix \"**/*.{ts,js}\"",
    "start": "esno src/index.ts",
    "test": "vitest",
    "release": "bumpp"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.25.2",
    "bumpp": "^8.2.1",
    "cac": "6.7.12",
    "eslint": "8.20.0",
    "esno": "0.16.3",
    "husky": "4.3.8",
    "typescript": "4.7.4",
    "unbuild": "0.7.6",
    "vite": "3.0.2",
    "vitest": "0.18.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
